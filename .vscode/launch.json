{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "attach",
			"name": "Docker: Attach to Node",
			"remoteRoot": "/app",
			"localRoot": "${workspaceFolder}",
			"port": 9229,
			"address": "localhost",
			"skipFiles": ["<node_internals>/**"]
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Launch Program (Local)",
			"program": "${workspaceFolder}/src/server.js",
			"skipFiles": ["<node_internals>/**"]
		}
	]
}
